    {
      "description": "focus space 1",
      "manipulators": [
        {
          "from": {
            "key_code": "1",
            "modifiers": {
              "mandatory": [
                "left_command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "echo ran > /tmp/asdf.ran.yaml && yabai -m space --focus 1"
            }
          ],
          "type": "basic"
        }
