    {
      "description": "change layout to bsp",
      "manipulators": [
        {
          "from": {
            "key_code": "a",
            "modifiers": {
              "mandatory": [
                "control",
                "option"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m space --layout bsp"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "change layout to stack",
      "manipulators": [
        {
          "from": {
            "key_code": "s",
            "modifiers": {
              "mandatory": [
                "control",
                "option"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m space --layout stack"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "change layout to float",
      "manipulators": [
        {
          "from": {
            "key_code": "d",
            "modifiers": {
              "mandatory": [
                "control",
                "option"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m space --layout stack"
            }
          ],
          "type": "basic"
        }
      ]
    },

    {
      "description": "switch application forwards",
      "manipulators": [
        {
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [
                "command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --focus stack.next || /usr/local/bin/yabai -m window --focus stack.first || /usr/local/bin/yabai -m window --focus next || /usr/local/bin/yabai -m window --focus first"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "switch application backwards",
      "manipulators": [
        {
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": [
                "command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --focus stack.prev || /usr/local/bin/yabai -m window --focus stack.last || /usr/local/bin/yabai -m window --focus prev || /usr/local/bin/yabai -m window --focus last"
            }
          ],
          "type": "basic"
        }
      ]
    },

    {
      "description": "switch application forwards (non-fullscreen, bsp)",
      "manipulators": [
        {
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [
                "command",
                "shift"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --focus next || /usr/local/bin/yabai -m window --focus first"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "switch application backwards (non-fullscreen, bsp)",
      "manipulators": [
        {
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": [
                "command",
                "shift"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/yabai -m window --focus prev || /usr/local/bin/yabai -m window --focus last"
            }
          ],
          "type": "basic"
        }
      ]
    },

    {
      "description": "open alacritty",
      "manipulators": [
        {
          "from": {
            "key_code": "return_or_enter",
            "modifiers": {
              "mandatory": [
                "control"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/Applications/Alacritty.app/Contents/MacOS/alacritty -qq &"
            }
          ],
          "type": "basic"
        }
      ]
    }

  ]
