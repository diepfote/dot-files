                            {
                                "from": {
                                    "key_code": "caps_lock",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [{ "key_code": "left_control" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Disable F1 help",
